#define int mot1 = 1;
#define int mot2 = 2;
#define int mot3 = 3;
#define int mot4 = 4;
#define int forwardLocPWM = 70;
void setup() 
{
  Serial.begin(9600);
  pinMode(mot1, OUTPUT);
  pinMode(mot2, OUTPUT);
  pinMode(mot3, OUTPUT);
  pinMode(mot4, OUTPUT);
}

void loop() 
{
  //Milestone 5 Test 1: Moving forward:
  analogWrite(mot1, forwardLocPWM);
  analogWrite(mot2, forwardLocPWM);
  analogWrite(mot3, forwardLocPWM);
  analogWrite(mot4, forwardLocPWM);

}
